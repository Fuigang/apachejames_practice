<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="EmailboxMapper">

    <select id="findEmailboxSeqByEmailAndType" parameterType="map" resultType="int">
        SELECT emailbox_seq 
        FROM emailbox 
        WHERE member_email = #{member_email} AND emailbox_type = #{emailbox_type}
    </select>

</mapper>