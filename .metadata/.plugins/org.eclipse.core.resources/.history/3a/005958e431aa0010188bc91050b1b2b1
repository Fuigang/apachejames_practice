<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="EmailSenderMapper">

    <insert id="saveSender" parameterType="EmailSenderDto">
        INSERT INTO email_sender (
            sender_seq, 
            email_seq, 
            sender_email
        ) VALUES (
            sender_seq.NEXTVAL, 
            #{email_seq}, 
            #{sender_email}
        )
    </insert>

</mapper>